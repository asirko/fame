    "esModuleInterop": false,
    "target": "es6",
    "types": ["reflect-metadata"],
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "dist",
    "experimentalDecorators": true,
