{
  "name": "fame",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:front\" \"npm run start:back\"",
    "start:front": "ng serve -o --proxy-config proxy-config.json",
    "start:back": "nodenv --env backend/.env --exec nodemon",
    "build": "npm run build:back && npm run build:front",
    "build:front": "ng build --prod",
    "prebuild:back": "rimraf ./dist/backend/",
    "build:back": "tsc -p tsconfig.server.json",
    "postbuild:back": "cpy backend/resources dist/backend/resources",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ts-node": "ts-node"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/platform-server": "^6.0.4",
    "@angular/router": "^6.0.3",
    "@types/socket.io": "^1.4.36",
    "@types/socket.io-client": "^1.4.32",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "rxjs": "^6.0.0",
    "socket.io": "^2.1.1",
    "ts-loader": "^4.4.1",
    "uws": "^10.148.1",
    "winston": "^3.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.5.6",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.5.1",
    "cpy-cli": "^2.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.18.3",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.0.6"
  }
}
